  "dependencies": {
  },
  "devDependencies": {
    "docsify-cli": "^4.3.0",
    "husky": "3.1.0",
    "textlint": "^11.3.1",
    "textlint-filter-rule-whitelist": "^2.0.0",
    "textlint-rule-no-todo": "^2.0.1",
    "textlint-rule-preset-ja-spacing": "^2.0.1",
    "textlint-rule-preset-jtf-style": "^2.3.3"
  }
